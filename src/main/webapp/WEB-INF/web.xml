<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <display-name>Archetype Created Web Application</display-name>
    <welcome-file-list>
        <welcome-file>main.do</welcome-file>
    </welcome-file-list>


    <context-param>
        <description>Image path</description>
        <param-name>images_path</param-name>
        <param-value>/server/file_storage/images</param-value>
    </context-param>
  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
    <welcome-file>main.do</welcome-file>
  </welcome-file-list>
  <error-page>
    <error-code>404</error-code>
    <location>/404.jsp</location>
  </error-page>
  <error-page>
    <exception-type>ua.nure.uvarov.exceptions.NotFoundException</exception-type>
    <location>/404.jsp</location>
  </error-page>
</web-app>
